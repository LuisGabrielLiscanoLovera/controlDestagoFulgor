<NuevoIntegrante>:
    GridLayout:
        cols: 6
        rows: 6
    BoxLayout:
		orientation: 'vertical'
		Label:
			text:"Agregar nuevo Integrante"
			pos_hint: {'right': 1}
		Label:
			text: 'fecha '
			pos_hint: {'left': 1}
		TextInput:
			hint_text: 'Nombre Completo'
			id: Nombre_Completo
			text: 'Monoline4 textinput'
		TextInput:
			hint_text: 'Numero de Documento'
			text: 'Monoline5 textinput'
			id: Num_Documento
		TextInput:
			hint_text: 'Foto Integrante'
			id:Foto_Integrante
			text: 'Monoline6 textinput'
		TextInput:
			hint_text: 'Foto Firma'
			id:Foto_Firma
			text: 'Monoline textinput'	
		Button:
			text: 'Guardar'
			pos_hint: {'right': 1}
			on_press:root.integrante(Nombre_Completo.text,Num_Documento.text,Foto_Integrante.text,Foto_Firma.text)
		Button:
			text: 'Cancelar '
			pos_hint: {'left': 1}
			on_press: root.manager.current = 'menu'